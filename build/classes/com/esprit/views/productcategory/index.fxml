<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="575.0" prefWidth="857.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esprit.controllers.productcategory.IndexController">
   <children>
      <TableView fx:id="categories" layoutX="234.0" layoutY="132.0" onMouseClicked="#SelectItem" prefHeight="315.0" prefWidth="517.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="516.0" text="Categorie" />
        </columns>
      </TableView>
      <Button layoutX="234.0" layoutY="548.0" mnemonicParsing="false" onAction="#redirectTonewCategory" prefHeight="31.0" prefWidth="249.0" style="-fx-background-color: #5cb85c;" text="+ Ajouter une nouvelle categorie" textFill="WHITE" />
      <Button layoutX="234.0" layoutY="88.0" mnemonicParsing="false" onAction="#Delete" prefHeight="31.0" prefWidth="111.0" style="-fx-background-color: #fb3640;" text="Supprimer" textFill="WHITE" />
      <Button layoutX="501.0" layoutY="548.0" mnemonicParsing="false" onAction="#Edit" prefHeight="31.0" prefWidth="127.0" style="-fx-background-color: #f0ad4e;" text="Modifier" textFill="WHITE" />
      <Label layoutX="233.0" layoutY="27.0" prefHeight="49.0" prefWidth="344.0" text="Liste des Categories">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <VBox alignment="TOP_CENTER" layoutY="7.0" prefHeight="585.0" prefWidth="200.0" style="-fx-background-color: white;">
         <children>
            <Button mnemonicParsing="false" onAction="#showCategory" prefHeight="42.0" prefWidth="186.0" text="Categorie" />
            <Label />
            <Button mnemonicParsing="false" onAction="#showProduct" prefHeight="42.0" prefWidth="186.0" text="Produit" textAlignment="CENTER" />
         </children>
      </VBox>
      <TextField fx:id="tfName" layoutX="368.0" layoutY="499.0" />
      <Label layoutX="244.0" layoutY="503.0" text="Nom de la Category " />
   </children>
</AnchorPane>
